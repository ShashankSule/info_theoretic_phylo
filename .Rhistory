x_term = -p_y[i]*log2(p_x[i])
}
I <- I + x_term + y_term
}
if(J == "ig"){
#information gain
if(p_y[i] == 0){
y_term = 0
} else{
y_term = -p_y[i]*log2(p_y[i])
}
if(p_x[i] == 0){
x_term = 0
} else {
x_term = -p_x[i]*log2(p_x[i])
}
if(p_xy[i] == 0){
xy_term = 0
} else {
xy_term = -p_xy[i]*log2(p_xy[i])
}
I <- I + xy_term - p_a*x_term - p_b*y_term
}
}
return(I)
}
