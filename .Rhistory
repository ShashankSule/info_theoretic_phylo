p_a <- length(A)/length(sequence)
# Computing p(y)
p_y <- base.freq(as.DNAbin(B))
p_b <- length(B)/length(sequence)
h_xy <- 0
h_x <- 0
h_y <- 0
# Computing p(x,y)
I <- 0
for(i in c(1:4)){
if(p_xy[i] !=0){
