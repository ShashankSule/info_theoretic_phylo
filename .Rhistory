}
if(p_x[i] != 0){
h_x <- h_x -p_x[i]*log2(p_x[i])
}
if(p_y[i] != 0){
h_y <- h_y -p_y[i]*log2(p_y[i])
}
}
I <- h_xy - p_a*h_x - p_b*h_y
return(I)
}
